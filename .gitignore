# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/jetty/*.log
/jetty/logs/*
/tmp

# Ignore index test files and generated config
/jetty/solr/collection1/data

/config/*.local.yml
/config/*.local.rb
/config/environments/*.local.yml
/config/environments/*.local.rb

# Ignore private config files
config/initializers/secret_token.rb

# Ignore vim .swp files
.*.swp
.DS_Store
.powrc

features/cassettes
spec/cassettes
coverage
public/assets
